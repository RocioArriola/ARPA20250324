@model ARPA20250324.AppWebMVC.Models.Warehouse

@{
    ViewData["Title"] = "Delete";
}

<h1 class="display-4 text-center" style="font-family:'Cambria Math'">Eliminar Bodega</h1>


<div>
    <h4 style="font-family:'Cambria Math'" class="text-secondary text-center">Bodegas</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 fw-bold">
            @Html.DisplayNameFor(model => model.WarehouseName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseName)
        </dd>
        <dt class="col-sm-2 fw-bold">
            @Html.DisplayNameFor(model => model.Notes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Notes)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger btn-sm" /> 
        <a asp-action="Index" class="btn btn-dark">Volver</a>
    </form>
</div>
